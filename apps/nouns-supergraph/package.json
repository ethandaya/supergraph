    "@heaps/engine": "workspace:*",
    "abitype": "^0.3.0",
    "bun-types": "^0.5.0",
    "parquetjs": "^0.11.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@heaps/cli": "workspace:*",
    "@heaps/tsconfig": "workspace:*",
    "@types/jest": "^29.2.4",
    "@types/node": "^17.0.12",
    "eslint-config-heaps": "workspace:*",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
